<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Ambiente" table="ambiente">
		<id name="idAmbiente" column="idAmbiente">
			<generator class="identity"></generator>
		</id>
		
		<property name="nroDeAmbiente" column="nrodeambiente" type="int"></property>
		<property name="ambiente" column="ambiente" type="string"></property>
		
	</class>
</hibernate-mapping>